   "source": [
    "n = int(input())\n",
    "\n",
    "li = []\n",
    "for _ in range(n) :\n",
    "    x, y = map(int, input().split())\n",
    "    li.append((x, y))\n",
    "li.sort(key=lambda x: (x[1], x[0]))\n",
    "for i in li :\n",
    "    print(i[0], i[1])"
   ]
